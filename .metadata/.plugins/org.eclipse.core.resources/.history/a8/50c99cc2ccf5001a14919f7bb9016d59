package Klase;

import java.time.LocalDate;
import java.util.ArrayList;
import java.util.List;

public class TopListaRecepata {
	private LocalDate datum;
	private List<Recept>recepti;

	//obicni kontruktor, kontruktor koji dobija recepte, i recepte i datum
	//
	public TopListaRecepata() {
		recepti = new ArrayList<Recept>();
		datum = LocalDate.now();
	}
	public TopListaRecepata(ArrayList<Recept> rec) {
		recepti = rec;
		datum = LocalDate.now();
	}
	
	public LocalDate getDatum() {
		return datum;
	}
	public void setDatum(LocalDate datum) {
		this.datum = datum;
	}
	public List<Recept> getRecepti() {
		return recepti;
	}
	public void setRecepti(List<Recept> recepti) {
		this.recepti = recepti;
	}
	
	
}
